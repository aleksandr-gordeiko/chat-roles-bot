{
  "extends": "airbnb-typescript/base",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-restricted-syntax": ["off", "ForOfStatement", "ForInStatement"],
    "guard-for-in": "off",
    "max-len": ["error", {"code":  120}],
    "no-continue": "off",
    "prefer-destructuring": "off"
  }
}
